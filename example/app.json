{
  "expo": {
    "name": "PAYJP-Example",
    "slug": "PAYJP-Example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "jp.pay.example",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jp.pay.example.PAYJPExample",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes."
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.jp.pay.example2"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "jp.pay.example.PAYJPExample"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "jp.pay.example://"
        }
      ],
      "@config-plugins/detox",
      "./config/add-android-dependencies",
      "./config/payjp-three-d-secure"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}